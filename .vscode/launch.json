{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Run SMDB.bat (terminal)",
            "type": "node-terminal",
            "request": "launch",
            "command": "cmd.exe /d /c \"${workspaceFolder}\\SMDB.bat\"",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Bash: Run SMDB.sh",
            "type": "bashdb",
            "request": "launch",
            "program": "${workspaceFolder}/SMDB.sh",
            "cwd": "${workspaceFolder}",
            "args": [],
            "env": {},
            "console": "integratedTerminal"
        }
    ]
}
